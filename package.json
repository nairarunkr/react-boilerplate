{
  "name": "bookstoreapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^3.3.7",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-scripts": "1.1.4"
  },
  "devDependencies": {
    "konfig": "latest",
    "babel-jest": "^19.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-deep-match": "^1.0.2",
    "chai-shallow-deep-equal": "^1.4.4",
    "chai-subset": "^1.5.0",
    "chai-things": "^0.2.0",
    "cucumber": "^2.0.0-rc.9",
    "enzyme": "^2.7.1",
    "extfs": "^0.0.7",
    "git-scripts": "^0.2.1",
    "glob": "^7.1.1",
    "jest": "^20.0.4",
    "jest-html-reporter": "^2.0.0",
    "jest-junit": "^1.4.0",
    "jest-sonar-reporter": "^1.1.0",
    "mountebank": "^1.10.0",
    "properties-reader": "^0.0.15",
    "react-router-test-context": "^0.1.0",
    "react-test-renderer": "^15.4.2",
    "recompose": "^0.25.0",
    "selenium-server": "~2.53.1",
    "selenium-standalone": "6.13.0",
    "sinon": "^2.1.0",
    "sonar-scanner": "^1.0.1",
    "wdio-cucumber-framework": "^0.2.16",
    "wdio-junit-reporter": "^0.3.0",
    "wdio-selenium-standalone-service": "^0.0.8",
    "wdio-spec-reporter": "^0.1.0",
    "webdriver-manager": "^12.0.4",
    "webdriverio": "^4.7.1",
    "webpack-dev-server": "^2.4.1"
  },
  "scripts": {
    "start-real": "NODE_ENV=real react-scripts start",
    "start-virtual": "NODE_ENV=virtual react-scripts start",
    "build": "webpack",
    "test": "standard && jest",
    "eject": "react-scripts eject",
    "fix": "standard --fix",
    "start-docs": "cd app/e2e/publish && bower install && polymer serve --port 5005",
    "docs": "open -a \"Google Chrome\" './app/e2e/publish/output/initialize.md'",
    "e2etest": "node ./app/e2e/gherkin-properties.js && standard && wdio ./app/config/wdio/wdio.conf.js",
    "e2etest-ci": "NODE_ENV=ci standard && wdio ./app/config/wdio/wdio.CI.conf.js",
    "sonar": "SONAR_SCANNER_OPTS=\"-Dproject.settings=sonar-scanner.properties\" node_modules/sonar-scanner/bin/sonar-scanner",
    "selenium-install": "rm -rf ./node_modules/selenium-standalone/.selenium && cp -f -R ./selenium ./node_modules/selenium-standalone/.selenium",
    "selenium": "selenium-standalone start",
    "coverage": "istanbul check-coverage"
  }
}
